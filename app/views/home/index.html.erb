    </section>    
    
        <!-- Team Section -->
    <section id="distributor" style="padding-bottom: 25px;" class="bg-light-gray">
        <div class="container">
            <div class="row">
            <h2 class="section-heading" style="text-align:center;">Distributors Enquiry</h2>
            <img style="width:100%" src="/assets/distributor.png"/>
            </div>
            <div class="row" style="padding-top: 20px;">
               <div class="col-lg-12">
                  <div class="row">
                    <div class="col-md-6">
                        <form id="applyForm" >
                            <div class="form-group">
                                <input type="text" class="form-control" placeholder="By *" id="apply_for" required data-validation-required-message="By">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control" placeholder="Name *" id="name" required data-validation-required-message="Please enter your name.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <input type="email" class="form-control" placeholder="Your Email *" id="email" required data-validation-required-message="Please enter your email address.">
                                <p class="help-block text-danger"></p>
                            </div>
                           <div class="form-group">
                                <input type="text" class="form-control" placeholder="Mobile No *" id="mobile_no" required data-validation-required-message="Please enter your mobile number.">
                                <p class="help-block text-danger"></p>
                            </div>
                           <div class="form-group">
                                <input type="text" class="form-control" placeholder="City *" id="curr_org" required data-validation-required-message="Please enter your city.">
                                <p class="help-block text-danger"></p>
                            </div>
                           <div class="form-group">
                                <input type="text" class="form-control" placeholder="Address *" id="curr_profile" required data-validation-required-message="Please enter your address.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control" placeholder="Pin-code *" id="curr_profile" required data-validation-required-message="Please enter pin-code.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control" placeholder="Product *" id="curr_profile" required data-validation-required-message="Please enter your product.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control" placeholder="Model *" id="curr_profile" required data-validation-required-message="Please enter product model.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control" placeholder="Complaint *" id="curr_profile" required data-validation-required-message="Please enter your complaint">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="text-center">
                                <div id="success"></div>
                                <button type="submit" class="btn btn-primary">Send</button>
                            </div>                           
                        </form>    
                    </div>
                    <div class="col-md-6">
                      <img src="/assets/distri.png	" style="width:100%" />
                    </div>
                  </div>
               </div>
            </div>
        </div>
    </section>
    
    <!-- Team Section -->
    <section id="carrier" style="padding-bottom: 25px;" class="bg-light-gray">
