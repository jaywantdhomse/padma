                    <h2 class="section-heading">Products</h2>
                    <h3 class="section-subheading text-muted">Lorem ipsum dolor sit amet consectetur.</h3>
                </div>
            </div>
            <div class="row">
            <% @categories.each do |category| %>
                <div class="col-md-3 col-sm-6 portfolio-item">
                
                	<% if !current_user.nil? && current_user.admin %>
		                <a href="#" class="portfolio-link add_product" data-toggle="modal">
		                    <div class="portfolio-hover">
		                        <div class="portfolio-hover-content">
		                            <i class="fa fa-plus fa-3x"></i>
		                        </div>
		                    </div>
		                    <img src="/assets/startup-framework.png" class="img-responsive" alt="">
		                </a>
                    <% else %>
                    	<a href="#" data-id="<%= category.id %>" data-name="<%= category.name %>" class="portfolio-link check_all_category" data-toggle="modal">
		                    <div class="portfolio-hover">
		                        <div class="portfolio-hover-content">
		                            <i class="fa fa-plus fa-3x"></i>
		                        </div>
		                    </div>
