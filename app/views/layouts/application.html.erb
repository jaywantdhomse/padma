                    	<div>
                    		<form action="<%= user_password_path %>" id="forgot-password-form">
                    			<div class="form-group">
		                            <input type="email" data-lable="Email" class="form-control" name="user[email]" placeholder="Email" id="user_email" style="width: 348px;margin: 0 0 0 63px;">
		                        </div>
		                         <div class="row">
						            <div class="text-center" style="padding: 12px 6px 5px 5px; font-size: 14px;">
						                <button type="submit" class="btn btn-primary">Submit</button>
						            </div>
						            <div class="text-center" style="color: green; text-align: center;" id="send_mail_success"></div>
						        </div>
                    		</form>
                    	</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
