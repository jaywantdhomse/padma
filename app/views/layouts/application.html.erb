<div class="modal fade" id="loginModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
	<div class="modal-dialog" style="width:900px">
		<div class="modal-content">
		    <div class="modal-header" align="center">
		        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
		            <div class="close"  data-dismiss="modal aria-hidden="true" style="opacity: 1;">&times;</div>
		        </button>
		    </div>
		    <div class="modal-body">
		        <div class="row">
		            <div class="col-lg-8">
		                <form accept-charset="UTF-8" action="<%= user_registration_path %>" class="user_signup" id="user_signup" method="post">
		                <div class="row">
		                    <h4 style="text-align: center">Register Now</h4>
		                    <div class="col-md-6">
		                        <div class="form-group">
		                            <input type="text" data-lable="First Name" class="form-control" name="user[first_name]" placeholder="First Name" id="" style="width:250px">
		                        </div>
		                        <div class="form-group">
		                            <input type="email" data-lable="Email" class="form-control" name="user[email]" placeholder="Email" id="user_email" style="width:250px">
		                        </div>
		                        <div class="form-group">
		                            <input type="password" data-lable="Password" class="form-control" name="user[password]" placeholder="Password" id="password" style="width:250px">
		                        </div>                                         
		                    </div>
		                    <div class="col-md-6">
		                        <div class="form-group">
		                            <input type="text" data-lable="Last Name" class="form-control" name="user[last_name]" placeholder="Last Name" id="" style="width:250px">
		                        </div>
		                        <div class="form-group">
		                            <input type="text" data-lable="Mobile No" class="form-control" name="user[mob_no]" placeholder="Mobile No" id="mob_no" style="width:250px">
		                        </div>
		                        <div class="form-group">
		                            <input type="password" data-lable="Confirm Password" class="form-control" placeholder="Confirm Password" id="confirm_password" style="width:250px">
		                        </div>                                         
		                    </div>
		                </div>
		                <div class="row">
		                    <div class="text-center">
		                        <button type="submit" class="btn btn-primary">Submit</button>
		                    </div>
		                    <div class="tet-center" style="color: green; text-align: center;" id="signup_success"></div>
		                </div>
		                </form>
		            </div>
		            <div class="col-md-4">
		                <form accept-charset="UTF-8" action="<%= user_session_path %>" class="user_login" id="user_login" method="post">
		                <h4 style="text-align: center">Already Register</h4>
		                <div class="form-group text-center">
		                    <input type="text" class="form-control" name="user[email]" id="email" placeholder="username" id="">
		                </div>
		                <div class="form-group text-center">
		                    <input type="password" class="form-control" name="user[password]" id="password" placeholder="password" id="">
		                </div>
		                <div class="text-center">
		                    <button type="submit" class="btn btn-primary">Submit</button>
		                </div>
		                </form>
		                <div class="text-center">
		                    <a style="color: blue;" type="submit" class="forgotpassword" data-toggle="modal" href="#">Forgot password</a>
		                </div>
		            </div>
		        </div>
		    </div>
		</div>
	</div>
</div>


