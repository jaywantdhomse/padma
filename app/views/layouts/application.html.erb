<!DOCTYPE html>
<html>
<head>
  <title>Padma</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <script src="/assets/jquery.js"></script>
    <script src="/assets/jquery_form.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="/assets/bootstrap.js"></script>
    <script src="/assets/agency.js"></script>

    <!-- Plugin JavaScript -->
    <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>
    <script src="/assets/classie.js"></script>
    <script src="/assets/cbpAnimatedHeader.js"></script>
    <link href="/assets/agency.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="/assets/font-awesome.min.css" rel="stylesheet" type="text/css">

    <!-- Contact Form JavaScript -->
<!--    <script src="js/jqBootstrapValidation.js"></script>
    <script src="js/contact_me.js"></script>-->
    <!-- Custom Theme JavaScript -->
    <script>
        var root_url = <%= root_url.to_json.html_safe %>;
    </script>
</head>
<body>
	<div class="container sub-header">
		<div class="row">
			<div class="col-lg-12" style="">
				<div class="col-lg-1 sub-header" style="font-size:22px; width: 109px;">PADMA <span style="font-size: 16px;">Qoute</span></div>
				<div class="col-lg-4 sub-header" style="text-align: center; border-left: 2px solid lightblue; height:60px;">Customer- 11111111111</div>
				<div class="col-lg-4 sub-header" style="text-align: center; border-left: 2px solid lightblue; height:60px;">Service Requirement</div>
				<div class="col-lg-1 sub-header" style="border-left: 2px solid lightblue; height:60px; width: 170px; padding-left: 37px;">Order Online</div>
				<div class="col-lg-1" style="float: right; width: 101px;"><a href="#"> <img src="/assets/logo.png" style="width:85%" /> </a></div>
			</div>
		</div>
	</div>
	<div class="container">
		<div class="row">
			<div class="col-lg-12" style="">
				<div class="col-lg-1 header"><a href="/home">Home</a></div>
				<div class="col-lg-2 header"><a href="/about_us">About Us</a></div>
				<div class="col-lg-2 header">Products</div>
				<div class="col-lg-2 header"><a href="/distributor">Distributor Enquiry</a></div>
				<div class="col-lg-2 header"><a href="/applicant">Carrier</a></div>
				<div class="col-lg-2 header"><a href="/contact">Contact Us</a></div>
				<div class="col-lg-1 header">
				<% if user_signed_in? %>
                    <a class="login" href="<%= destroy_user_session_url %>" >Logout</a>
                <% else %>
                    <a class="login" href="#" >Login</a>
                <% end%></div>
			</div>
		</div>
	</div>
	<% flash.each do |name, msg| %>
    <%= content_tag :div, msg, class: "alert alert-info" %>
  <% end %>
	
<%= yield %>


        <div class="container">
            <div class="row" style="line-height: 28px;">
            	<div class="col-lg-12">
		            <div class="col-md-6" style="background: dimgrey; height: 115px;">
		                <span class="copyright" style="display:block; color:blue; font-size:25px; font-weight:bold;">Suvidhinath Electronics Pvt. Ltd.</span>
		                <span class="copyright" style="display:block; color:blue;">Reg Office: Alco Market, Rajiv Nagar,</span>
		                <span class="copyright" style="display:block;"><span style="font-weight: bold;">Nashik -</span> 422008(MH), <span style="font-weight: bold;">Mob -</span> 08600007205</span>
		                <span class="copyright"><span style="font-weight: bold;">Email -</span> info@seplworld.in, <span style="font-weight: bold;">Website - </span><a style="color:blueviolet;" href="http://seplworld.in" target=_blank?>www.seplworld.in</a></span>
		                
		            </div>
		            <div class="col-md-6" style="background: dimgrey; height: 115px; text-align: center;
    padding-top: 18px;">
		                <ul class="list-inline quicklinks">
		                    <li><a href="#">Privacy Policy</a>
		                    </li>
		                    <li><a href="#">Terms of Use</a>
		                    </li>
		                    <ul class="list-inline social-buttons">
		                    <li><a href="#"><i class="fa fa-twitter"></i></a>
		                    </li>
		                    <li><a href="#"><i class="fa fa-facebook"></i></a>
		                    </li>
		                    <li><a href="#"><i class="fa fa-linkedin"></i></a>
		                    </li>
		                </ul>
		                </ul>
		                <ul class="list-inline quicklinks">
							<span class="copyright">Copyright &copy; padma.com 2016</span>
		                </ul>
		                </div>
		         </div>   
            </div>
        </div>
        
		<div class="modal fade" id="loginModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
			<div class="modal-dialog" style="width:900px">
				<div class="modal-content">
				    <div class="modal-header" align="center">
				        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
				            <div class="close"  data-dismiss="modal aria-hidden="true" style="opacity: 1;">&times;</div>
				        </button>
				    </div>
				    <div class="modal-body">
				        <div class="row">
				            <div class="col-lg-8">
				                <form accept-charset="UTF-8" action="<%= user_registration_path %>" class="user_signup" id="user_signup" method="post">
				                <div class="row">
				                    <h4 style="text-align: center">Register Now</h4>
				                    <div class="col-md-6">
				                        <div class="form-group">
				                            <input type="text" data-lable="First Name" class="form-control" name="user[first_name]" placeholder="First Name" id="" style="width:250px">
				                        </div>
				                        <div class="form-group">
				                            <input type="email" data-lable="Email" class="form-control" name="user[email]" placeholder="Email" id="user_email" style="width:250px">
				                        </div>
				                        <div class="form-group">
				                            <input type="password" data-lable="Password" class="form-control" name="user[password]" placeholder="Password" id="password" style="width:250px">
				                        </div>                                         
				                    </div>
				                    <div class="col-md-6">
				                        <div class="form-group">
				                            <input type="text" data-lable="Last Name" class="form-control" name="user[last_name]" placeholder="Last Name" id="" style="width:250px">
				                        </div>
				                        <div class="form-group">
				                            <input type="text" data-lable="Mobile No" class="form-control" name="user[mob_no]" placeholder="Mobile No" id="mob_no" style="width:250px">
				                        </div>
				                        <div class="form-group">
				                            <input type="password" data-lable="Confirm Password" class="form-control" placeholder="Confirm Password" id="confirm_password" style="width:250px">
				                        </div>                                         
				                    </div>
				                </div>
				                <div class="row">
				                    <div class="text-center">
				                        <button type="submit" class="btn btn-primary">Submit</button>
				                    </div>
				                    <div class="tet-center" style="color: green; text-align: center;" id="signup_success"></div>
				                </div>
				                </form>
				            </div>
				            <div class="col-md-4">
				                <form accept-charset="UTF-8" action="<%= user_session_path %>" class="user_login" id="user_login" method="post">
				                <h4 style="text-align: center">Already Register</h4>
				                <div class="form-group text-center">
				                    <input type="text" class="form-control" name="user[email]" id="email" placeholder="username" id="">
				                </div>
				                <div class="form-group text-center">
				                    <input type="password" class="form-control" name="user[password]" id="password" placeholder="password" id="">
				                </div>
				                <div class="text-center">
				                    <button type="submit" class="btn btn-primary">Submit</button>
				                </div>                                        
				                </form>
				            </div>                            
				        </div>
				    </div>
				</div>
			</div>
		</div>
</body>
</html>
